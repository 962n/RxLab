<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.userEdit.UserEditFragment">
    <LinearLayout
        android:orientation="vertical"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Button
            android:text="Add random 1 user"
            android:id="@+id/buttonAdd1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button
            android:text="Add random 10 user"
            android:id="@+id/buttonAdd10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button
            android:text="Delete random 1 user"
            android:id="@+id/buttonDelete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button
            android:text="Delete all user"
            android:id="@+id/buttonDeleteAll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
